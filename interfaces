auto lo
iface lo inet loopback

auto wlp5s0
iface wlp5s0 inet static
  address 192.168.1.100
  netmask 255.255.255.0
  gateway 192.168.1.1

wpa-ssid TIM-18378133
wpa-psk ANDREAELUCADC

auto enp6s0
iface enp6s0 inet manual
up ip link set dev enp6s0up
down ip link set dev enp6s0down

dns-nameserver 192.168.1.1
dns-nameserver 8.8.8.8
dns-nameserver 85.37.17.51
